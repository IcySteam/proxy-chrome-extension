{
  "manifest_version": 2,
  "name": "MarsProxies Session Randomizer Extension",
  "version": "28",
  "options_page": "options.html",
  "options_ui": {
    "chrome_style": true,
    "page": "options.html"
  },
  "permissions": [
    "tabs",
    "proxy",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "webNavigation",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts":
    [
      "js/background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "MarsProxies Session Randomizer Extension",
    "default_popup": "options.html"
  }
}
